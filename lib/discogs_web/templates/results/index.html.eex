<!-- the template for the find results index page -->
<table>
  <tr>
    <th>Artist</th>
    <th>Title</th>
    <th>Label</th>
    <th>Format</th>
    <th>Catalogue Number</th>
    <th>Link</th>
  </tr>
  <%= for item <- @items do %>
    <tr>
      <%= render "item.html", item: item %>
    </tr>
  <% end %>
</table>
